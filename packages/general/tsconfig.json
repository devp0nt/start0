{
  "include": [
    "src",
    "../webapp/src/types",
    "../backend/src/types"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/general/*": [
        "./*"
      ],
      "@/backend/*": [
        "../backend/*"
      ],
      "@/webapp/*": [
        "../webapp/*"
      ]
    },
    "outDir": "./dist",
    "declarationMap": false,
    "declaration": false,
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "inlineSources": true,
    "isolatedModules": false,
    "jsx": "react-jsx",
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "tsc-alias": {
    "verbose": false,
    "resolveFullPaths": true,
    "fileExtensions": {
      "inputGlob": "{js,jsx,mjs}",
      "outputCheck": [
        "js",
        "json",
        "jsx",
        "mjs"
      ]
    }
  }
}