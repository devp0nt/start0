{
  "name": "@prisma/shared",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsgo --noEmit ./tsconfig.json",
    "build": "tsgo --build ./tsconfig.json",
    "watch": "tsgo --watch ./tsconfig.json",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma/shared && rimraf ./dist && rimraf ./src/generated",
    "build:clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma/shared && rimraf ./dist"
  },
  "dependencies": {
    "@shared/base": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
