{
  "name": "@prisma/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsgo --noEmit ./tsconfig.json",
    "build": "tsgo --build ./tsconfig.json",
    "watch": "tsgo --watch ./tsconfig.json",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma/backend && rimraf ./dist && rimraf ./src/generated && rimraf ../shared/src/generated",
    "prisma-migrate-dev": "bun run --env-file=./.env prisma migrate dev && bun run build",
    "prisma-migrate-reset": "bun run --env-file=./.env prisma migrate reset",
    "prisma-migrate-test": "bun run --env-file=./.env.test prisma migrate deploy",
    "prisma-migrate-prod": "bun run prisma migrate deploy",
    "prisma-generate-client": "bun run prisma generate client && bun run build",
    "build:clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma/backend && rimraf ./dist"
  },
  "dependencies": {
    "@devp0nt/tri0": "^1.0.0-next.36",
    "@prisma/client": "^6.14.0",
    "@prisma/shared": "workspace:*",
    "@shared/base": "workspace:*",
    "exponential-backoff": "^3.1.2",
    "prisma": "^6.14.0",
    "prisma-json-types-generator": "^3.6.0",
    "prisma-zod-generator": "^1.21.3"
  },
  "devDependencies": {
    "@prisma/generator-helper": "^6.17.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
