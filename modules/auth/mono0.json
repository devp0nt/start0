[
  {
    "name": "@auth/shared",
    "preset": ["sharedBaseModule", "sharedNested"],
    "deps": ["@prisma/shared"]
  },
  {
    "name": "@auth/backend",
    "preset": ["backendBaseModule", "backendNested"],
    "deps": ["@auth/shared", "@backend/shared", "@prisma/backend", "@shared/base"]
  },
  {
    "name": "@auth/site",
    "preset": ["siteBaseModule", "siteNested"],
    "deps": ["@auth/shared"]
  },
  {
    "name": "@auth/frontend-base",
    "preset": ["clientBaseModule", "clientBaseNested"],
    "deps": ["@auth/shared", "@auth/backend", "@shared/base"]
  },
  {
    "name": "@auth/admin",
    "preset": ["adminBaseModule", "adminNested"],
    "deps": ["@auth/frontend-base", "@admin/base"]
  }
]
