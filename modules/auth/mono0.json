[
  {
    "name": "@auth/shared",
    "preset": ["sharedBaseModule", "sharedNested"],
    "deps": ["@prisma/shared"]
  },
  {
    "name": "@auth/backend",
    "preset": ["backendBaseModule", "backendNested"],
    "deps": ["@auth/shared", "@backend/shared", "@prisma/backend", "@apps/shared"]
  },
  {
    "name": "@auth/site",
    "preset": ["siteBaseModule", "siteNested"],
    "deps": ["@auth/shared"]
  },
  {
    "name": "@auth/client-base",
    "preset": ["clientBaseModule", "clientBaseNested"],
    "deps": ["@auth/shared", "@auth/backend"]
  },
  {
    "name": "@auth/admin",
    "preset": ["adminBaseModule", "adminNested"],
    "deps": ["@auth/client-base", "@admin/base"]
  }
]
