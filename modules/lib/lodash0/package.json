{
  "name": "@devp0nt/lodash0",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf {{distDir}}"
  },
  "dependencies": {},
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "node": [
        "./src/index.ts",
        "./src/index.tsx"
      ]
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "node": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    }
  }
}
