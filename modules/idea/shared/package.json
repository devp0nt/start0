{
  "dependencies": {
    "@prisma0/backend": "workspace:*",
    "@devp0nt/route0": "workspace:*"
  },
  "name": "@idea/shared",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "development": {
        "import": "./src/index.ts",
        "types": "./src/index.ts"
