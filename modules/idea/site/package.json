{
  "name": "@idea/site",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@idea/site && rimraf ./dist",
    "watch:fast": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput"
  },
  "dependencies": {
    "@idea/shared": "workspace:*",
    "@site/core": "workspace:*"
  }
}
