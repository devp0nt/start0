{
  "dependencies": {
    "@site/core": "workspace:*"
  },
  "name": "@idea/site",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": [
        "src/index.ts",
        "src/index.tsx"
      ],
      "types": [
        "src/index.ts",
        "src/index.tsx"
      ]
    },
    "*": {
      "import": [
        "src/*.ts",
        "src/*.tsx"
      ],
      "types": [
        "src/*.ts",
        "src/*.tsx"
      ]
    }
  }
}
