{
  "name": "@idea/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@idea/backend && rimraf ./dist",
    "watch:fast": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput"
  },
  "dependencies": {
    "@backend/core": "workspace:*",
    "@idea/shared": "workspace:*",
    "@prisma0/backend": "workspace:*"
  }
}
