{
  "name": "@prisma0/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma0/backend && rimraf ./dist"
  },
  "dependencies": {
    "@idea/shared": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.js"
    }
  }
}
