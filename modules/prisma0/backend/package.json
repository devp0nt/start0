{
  "name": "@prisma0/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma0/backend && rimraf ./dist && rimraf ./src/generated",
    "prisma-migrate-dev": "bun run --env-file=./.env prisma migrate dev {{ARGS}}",
    "prisma-migrate-test": "bun run --env-file=./.env.test prisma migrate dev {{ARGS}}",
    "prisma-generate-client": "bun run prisma generate client {{ARGS}}",
    "lint": "eslint --cache --cache-location ../../../node_modules/.cache/eslint/.eslintcache ."
  },
  "dependencies": {},
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}