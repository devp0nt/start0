{
  "name": "@prisma0/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/@prisma0/backend && rimraf ./dist && rimraf ./src/generated",
    "prisma-migrate-dev": "bun run --env-file=./.env prisma migrate dev && bun run build",
    "prisma-migrate-test": "bun run --env-file=./.env.test prisma migrate dev",
    "prisma-generate-client": "bun run prisma generate client && bun run build",
    "lint": "eslint --cache --cache-location ../../../node_modules/.cache/eslint/.eslintcache ."
  },
  "dependencies": {}
}