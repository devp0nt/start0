{
  "name": "@prisma/backend",
  "preset": ["backendBaseModule", "cold"],
  "tags": ["prisma"],
  "deps": ["@prisma/shared", "@backend/base", "@auth/shared"],
  "packageJson": {
    "value": {
      "scripts": {
        "types": "tsc --noEmit --project ~/.tsconfig/{{unitName}}/tsconfig.json",
        "clean": "rimraf {{projectRootDir}}/node_modules/.cache/tsbuildinfo/{{unitName}} && rimraf {{distDir}} && rimraf ./src/generated && rimraf ../shared/src/generated"
      }
    }
  },
  "tsconfigs": {
    "core": {
      "settings": {}
    }
  }
}
