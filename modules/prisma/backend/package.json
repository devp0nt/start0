{
  "name": "@prisma/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../node_modules/.cache/tsbuildinfo/{{name}} && rimraf ./dist && rimraf ./src/generated && rimraf ../shared/src/generated",
    "prisma-migrate-dev": "bun run --env-file=./.env prisma migrate dev && bun run build",
    "prisma-migrate-reset": "bun run --env-file=./.env prisma migrate reset",
    "prisma-migrate-test": "bun run --env-file=./.env.test prisma migrate deploy",
    "prisma-migrate-prod": "bun run prisma migrate deploy",
    "prisma-generate-client": "bun run prisma generate client && bun run build && cd ../shared && bun run build",
    "lint": "eslint --cache --cache-location ../../../node_modules/.cache/eslint/@prisma/backend/.eslintcache ."
  },
  "dependencies": {
    "@devp0nt/tri0": "^1.0.0-next.31",
    "@prisma/client": "^6.14.0",
    "prisma": "^6.14.0",
    "prisma-json-types-generator": "^3.6.0",
    "prisma-zod-generator": "^1.21.3",
    "@prisma/shared": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
