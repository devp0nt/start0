{
  "name": "svagatron",
  "version": "0.1.0",
  "homepage": "https://github.com/iserdmi/svagatron",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iserdmi/svagatron.git"
  },
  "bugs": {
    "url": "https://github.com/iserdmi/svagatron/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "b": "pnpm --filter backend",
    "w": "pnpm --filter webapp",
    "g": "pnpm --filter general",
    "giw": "vscode-generate-index-standalone ./packages/backend/src/router/trpc/index.ts ./packages/backend/src/router/rest/index.ts ./packages/backend/src/services/other/emails/preview.ts ./packages/webapp/src/lib/icons.tsx -w ./packages/webapp/src/**/* -w ./packages/backend/src/**/* -w ./packages/general/src/**/*",
    "dev": "concurrently --raw 'pnpm giw' 'pnpm b dev' 'pnpm w dev'",
    "types": "concurrently --names 'backend,webapp,general' 'pnpm b types' 'pnpm w types' 'pnpm g types'",
    "lint": "concurrently --names 'backend,webapp,general' 'pnpm b lint' 'pnpm w lint' 'pnpm g lint'",
    "lint-fix": "concurrently --names 'backend,webapp,general' 'pnpm b lint-fix' 'pnpm w lint-fix' 'pnpm g lint-fix'",
    "test": "pnpm b test && pnpm w test && pnpm g test",
    "prettify": "pnpm -r --parallel prettify",
    "dcc": "dotenv -e ./deploy/.env.docker -e ./packages/backend/.env -e ./packages/webapp/.env -- bash -c 'docker compose -f ./deploy/docker-compose.local.yml config'",
    "dcb": "dotenv -e ./deploy/.env.docker -e ./packages/backend/.env -e ./packages/webapp/.env -- bash -c 'docker compose -f ./deploy/docker-compose.local.yml build'",
    "dcu": "dotenv -e ./deploy/.env.docker -e ./packages/backend/.env -e ./packages/webapp/.env -- bash -c 'docker compose -f ./deploy/docker-compose.local.yml up'",
    "sstage": "dotenv -e ./deploy/secrets/.env.settings.stage -- bash -c 'eval $(openv line -e ./deploy/secrets/.env.settings.stage -b \"heroku config:set \" -a \" --app $HEROKU_APP_NAME\")'",
    "sprod": "dotenv -e ./deploy/secrets/.env.settings.prod -- bash -c 'eval $(openv line -e ./deploy/secrets/.env.settings.prod -b \"heroku config:set \" -a \" --app $HEROKU_APP_NAME\")'",
    "prepare": "husky"
  },
