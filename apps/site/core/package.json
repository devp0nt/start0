{
  "dependencies": {},
  "name": "@site/core",
  "private": true,
  "exports": {
    ".": {
      "import": [
        "src/index.ts",
        "src/index.tsx"
      ],
      "types": [
        "src/index.ts",
        "src/index.tsx"
      ]
    },
    "components/*": {
      "import": [
        "src/components/*.ts",
        "src/components/*.tsx"
      ],
      "types": [
        "src/components/*.ts",
        "src/components/*.tsx"
      ]
    },
    "lib/*": {
      "import": [
        "src/lib/*.ts",
        "src/lib/*.tsx"
      ],
      "types": [
        "src/lib/*.ts",
        "src/lib/*.tsx"
      ]
    }
  },
  "type": "module"
}
