{
  "name": "@site/app",
  "preset": ["siteWithTsconfig", "app"],
  "deps": ["@site/core", "@devp0nt/env0", "#site"],
  "tsconfig": {
    "settings": {
      "setRootDirBySelfIncludes": false
    },
    "value": {
      "include": [".react-router/types/**/*", "./react-router.config.ts", "./vite.config.ts", "./src/**/*"],
      "compilerOptions": {
        "rootDir": "./",
        "rootDirs": ["./", "./.react-router/types"]
      }
    }
  },
  "packageJson": {
    "value": {
      "scripts": {
        "dev": "cross-env NODE_ENV=development bun run --bun vite",
        "types": "tsc --noEmit",
        "types:rr": "rimraf dist && react-router typegen && tsc",
        "build": "cross-env NODE_ENV=production react-router build",
        "start": "cross-env NODE_ENV=production bun ./dist/server/index.js"
      }
    }
  }
}
