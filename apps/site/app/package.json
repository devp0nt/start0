{
  "dependencies": {
    "@react-router/node": "catalog:",
    "isbot": "^5",
    "@devp0nt/env0": "workspace:*",
    "@devp0nt/gen0": "workspace:*",
    "@idea/site": "workspace:*",
    "@site/core": "workspace:*"
  },
  "name": "@site/app",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "cross-env NODE_ENV=development bun run --bun vite",
    "start": "cross-env NODE_ENV=production bun ./dist/server/index.js",
    "types": "rimraf dist && react-router typegen && tsc"
  },
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./pages/*": {
      "import": "./dist/pages/*.js",
      "types": "./dist/pages/*.d.ts"
    },
    "./routes/*": {
      "import": "./dist/routes/*.js",
      "types": "./dist/routes/*.d.ts"
    },
    "./routes/generated/*": {
      "import": "./dist/routes/generated/*.js",
      "types": "./dist/routes/generated/*.d.ts"
    }
  },
  "type": "module"
}
