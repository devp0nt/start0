{
  "dependencies": {
    "@devp0nt/gen0": "workspace:*",
    "@idea/site": "workspace:*",
    "@site/core": "workspace:*"
  },
  "name": "@site/app",
  "private": true,
  "exports": {
    "./*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./pages/*": {
      "import": "./dist/pages/*",
      "types": "./dist/pages/*"
    },
    "./routes/*": {
      "import": "./dist/routes/*",
      "types": "./dist/routes/*"
    },
    "./routes/generated/*": {
      "import": "./dist/routes/generated/*",
      "types": "./dist/routes/generated/*"
    }
  },
  "type": "module"
}
