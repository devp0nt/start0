{
  "dependencies": {
    "@devp0nt/gen0": "workspace:*",
    "@idea/site": "workspace:*",
    "@site/core": "workspace:*"
  },
  "name": "@site/app",
  "private": true,
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./dist/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "exports": {
    "./*": {
      "import": "./src/*",
      "types": "./src/*"
    },
    "./pages/*": {
      "import": "./src/pages/*",
      "types": "./src/pages/*"
    },
    "./routes/*": {
      "import": "./src/routes/*",
      "types": "./src/routes/*"
    },
    "./routes/generated/*": {
      "import": "./src/routes/generated/*",
      "types": "./src/routes/generated/*"
    }
  },
  "type": "module"
}
