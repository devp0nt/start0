{
  "dependencies": {
    "@devp0nt/env0": "workspace:*",
    "@devp0nt/gen0": "workspace:*",
    "@idea/site": "workspace:*",
    "@site/core": "workspace:*"
  },
  "name": "@site/app",
  "private": true,
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./dist/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "exports": {
    "./*": {
      "import": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*.js",
        "./src/*.jsx",
        "./src/*.json"
      ],
      "types": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*.js",
        "./src/*.jsx",
        "./src/*.json"
      ]
    },
    "./pages/*": {
      "import": [
        "./src/pages/*.ts",
        "./src/pages/*.tsx",
        "./src/pages/*.js",
        "./src/pages/*.jsx",
        "./src/pages/*.json"
      ],
      "types": [
        "./src/pages/*.ts",
        "./src/pages/*.tsx",
        "./src/pages/*.js",
        "./src/pages/*.jsx",
        "./src/pages/*.json"
      ]
    },
    "./routes/*": {
      "import": [
        "./src/routes/*.ts",
        "./src/routes/*.tsx",
        "./src/routes/*.js",
        "./src/routes/*.jsx",
        "./src/routes/*.json"
      ],
      "types": [
        "./src/routes/*.ts",
        "./src/routes/*.tsx",
        "./src/routes/*.js",
        "./src/routes/*.jsx",
        "./src/routes/*.json"
      ]
    },
    "./routes/generated/*": {
      "import": [
        "./src/routes/generated/*.ts",
        "./src/routes/generated/*.tsx",
        "./src/routes/generated/*.js",
        "./src/routes/generated/*.jsx",
        "./src/routes/generated/*.json"
      ],
      "types": [
        "./src/routes/generated/*.ts",
        "./src/routes/generated/*.tsx",
        "./src/routes/generated/*.js",
        "./src/routes/generated/*.jsx",
        "./src/routes/generated/*.json"
      ]
    }
  },
  "type": "module"
}
