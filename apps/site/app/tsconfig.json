{
  "extends": "../../../.mono0/tsconfig.site.json",
  "include": ["./src/**/*", ".react-router/types/**/*", "./react-router.config.ts", "./vite.config.ts"],
  "exclude": ["**/dist/**/*", "**/node_modules/**/*", "**/public/**/*"],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "noEmitOnError": false,
    "outDir": "./dist",
    "tsBuildInfoFile": "../../../node_modules/.cache/tsbuildinfo/@site/app/.tsbuildinfo",
    "rootDir": "./",
    "rootDirs": ["./", "./.react-router/types"],
    "paths": {
      "@site/app/*": ["./src/*"],
      "@idea/site/*": ["../../../modules/idea/site/src/*"],
      "@site/core/*": ["../core/src/*"],
      "@idea/shared/*": ["../../../modules/idea/shared/src/*"],
      "@backend/trpc-router": ["../../backend/router/trpc/src/index.ts"],
      "@backend/trpc-router/*": ["../../backend/router/trpc/src/*"]
    }
  },
  "references": [
    {
      "path": "../../../modules/idea/site/tsconfig.json"
    },
    {
      "path": "../core/tsconfig.json"
    },
    {
      "path": "../../../modules/idea/shared/tsconfig.json"
    },
    {
      "path": "../core/tsconfig.json"
    },
    {
      "path": "../../backend/router/trpc/tsconfig.json"
    }
  ]
}
