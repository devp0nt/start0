{
  "extends": "../../../../.mono0/tsconfig.backend.json",
  "include": ["./src/**/*"],
  "exclude": ["**/dist/**/*", "**/node_modules/**/*", "**/public/**/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "~/node_modules/.cache/tsbuildinfo/@backend/trpc-router/.tsbuildinfo",
    "paths": {
      "@backend/trpc-router/*": ["./src/*"],
      "@backend/core/*": ["../../core/src/*"]
    },
    "rootDir": "./src",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "./dist",
    "noEmitOnError": false,
    "declarationDir": "./dist"
  },
  "references": [
    {
      "path": "../../core/tsconfig.json"
    }
  ]
}
