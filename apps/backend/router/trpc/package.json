{
  "name": "@backend/trpc-router",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf ../../../../node_modules/.cache/tsbuildinfo/@backend/trpc-router && rimraf ./dist",
    "watch:fast": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput"
  },
  "dependencies": {
    "@appConfig/backend": "workspace:*",
    "@backend/core": "workspace:*",
    "@idea/backend": "workspace:*"
  }
}
