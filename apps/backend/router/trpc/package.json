{
  "name": "@backend/trpc-router",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf {{distDir}}"
  },
  "dependencies": {
    "@appConfig/backend": "workspace:*",
    "@backend/core": "workspace:*",
    "@idea/backend": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "node": [
        "./src/index.ts",
        "./src/index.tsx"
      ]
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "node": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    },
    "./ping/*": {
      "import": "./dist/ping/*.js",
      "types": "./dist/ping/*.d.ts",
      "node": [
        "./src/ping/*.ts",
        "./src/ping/*.tsx"
      ]
    }
  }
}
