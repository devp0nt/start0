{
  "name": "@backend/hono-router",
  "private": true,
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "development": "./src/*.js"
    },
    "./hello/*": {
      "import": "./dist/hello/*.js",
      "types": "./dist/hello/*.d.ts",
      "development": "./src/hello/*.js"
    },
    "./ping/*": {
      "import": "./dist/ping/*.js",
      "types": "./dist/ping/*.d.ts",
      "development": "./src/ping/*.js"
    }
  },
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf {{distDir}}"
  },
  "dependencies": {
    "@backend/core": "workspace:*"
  }
}
