{
  "dependencies": {
    "@backend/core": "workspace:*"
  },
  "name": "@backend/hono-router",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*",
      "types": "./src/*"
    },
    "./ping/*": {
      "import": "./src/ping/*",
      "types": "./src/ping/*"
    },
    "./hello/*": {
      "import": "./src/hello/*",
      "types": "./src/hello/*"
    }
  }
}
