{
  "name": "@backend/hono-router",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@backend/core": "workspace:*"
  },
  "exports": {
    ".": {
      "node": [
        "./src/index.ts",
        "./src/index.tsx"
      ],
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "node": [
        "./src/*.ts",
        "./src/*.tsx"
      ],
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./hello/*": {
      "node": [
        "./src/hello/*.ts",
        "./src/hello/*.tsx"
      ],
      "import": "./dist/hello/*.js",
      "types": "./dist/hello/*.d.ts"
    },
    "./ping/*": {
      "node": [
        "./src/ping/*.ts",
        "./src/ping/*.tsx"
      ],
      "import": "./dist/ping/*.js",
      "types": "./dist/ping/*.d.ts"
    }
  }
}
