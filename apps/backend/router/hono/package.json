{
  "dependencies": {
    "@backend/core": "workspace:*"
  },
  "name": "@backend/hono-router",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": [
        "src/index.ts",
        "src/index.tsx"
      ],
      "types": [
        "src/index.ts",
        "src/index.tsx"
      ]
    },
    "*": {
      "import": [
        "src/*.ts",
        "src/*.tsx"
      ],
      "types": [
        "src/*.ts",
        "src/*.tsx"
      ]
    },
    "hello/*": {
      "import": [
        "src/hello/*.ts",
        "src/hello/*.tsx"
      ],
      "types": [
        "src/hello/*.ts",
        "src/hello/*.tsx"
      ]
    },
    "ping/*": {
      "import": [
        "src/ping/*.ts",
        "src/ping/*.tsx"
      ],
      "types": [
        "src/ping/*.ts",
        "src/ping/*.tsx"
      ]
    }
  }
}
