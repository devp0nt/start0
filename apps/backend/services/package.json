{
  "dependencies": {
    "@backend/core": "workspace:*"
  },
  "name": "@backend/services",
  "private": true,
  "type": "module",
  "scripts": {
    "dev1": "bun --tsconfig-override tsconfig.json src/dev.ts --watch",
    "dev": "NODE_ENV=development bun src/dev.ts --watch"
  },
  "exports": {
    "./*": {
      "import": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*.js",
        "./src/*.jsx",
        "./src/*.json"
      ],
      "types": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*.js",
        "./src/*.jsx",
        "./src/*.json"
      ]
    }
  }
}
