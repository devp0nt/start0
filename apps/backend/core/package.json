{
  "name": "@backend/core",
  "private": true,
  "type": "module",
  "scripts": {
    "types": "tsc --noEmit",
    "build": "tsc --build",
    "watch": "tsc --watch --noEmitOnError false --skipLibCheck --noCheck --assumeChangesOnlyAffectDirectDependencies  --preserveWatchOutput",
    "clean": "rimraf {{distDir}}"
  },
  "dependencies": {
    "@devp0nt/lodash0": "workspace:*",
    "@prisma0/backend": "workspace:*"
  },
  "exports": {
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "types": "./dist/lib/*.d.ts",
      "node": [
        "./src/lib/*.ts",
        "./src/lib/*.tsx"
      ]
    }
  }
}
