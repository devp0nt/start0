      "@idea/admin/*": ["./modules/idea/*"],
      "@idea/backend/*": ["./modules/idea/*"],
      "@idea/shared/*": ["./modules/idea/*"],
      "@idea/site/*": ["./modules/idea/*"],
      "@site/app/*": ["./apps/site/app/*"],
      "@site/core/*": ["./apps/site/core/*"],
      "@tools/shared": ["./tools/src/index.ts"],
