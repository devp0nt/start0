      "@client/base/*": ["./apps/client/base/*"],
      "@cross/app/*": ["./apps/cross/app/*"],
      "@cross/base/*": ["./apps/cross/base/*"],
      "@cross/core/*": ["./apps/cross/core/*"],
      "@cross/ctx": ["./apps/cross/ctx/index.tsx"],
      "@cross/ctx/*": ["./apps/cross/ctx/*"],
      "@devp0nt/rjs0": ["./modules/rjs0/src/index.ts"],
