{
  "compilerOptions": {
    "types": ["node", "bun"],
    "paths": {
      "@admin/app": ["./apps/admin/app/src/index.tsx"],
      "@admin/app/*": ["./apps/admin/app/src/*"],
      "@admin/backend/*": ["./apps/admin/backend/*"],
      "@admin/base/*": ["./apps/admin/base/*"],
      "@admin/core/*": ["./apps/admin/core/*"],
      "@admin/ctx": ["./apps/admin/ctx/index.tsx"],
      "@admin/ctx/*": ["./apps/admin/ctx/*"],
      "@adminUser/admin/*": ["./modules/adminUser/*"],
