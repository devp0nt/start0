{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "semi": false,
  "printWidth": 120,
  "trailingComma": "all",
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["*.jsonc", "*.json"],
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
